<?php
	switch($action)
	{
		default:
			checkLogin($login, $pass);
			break;			
	}
	
function checkLogin($login, $pass)
{
	$js = new json_a();
	
	try
	{
		throw new Exception("Error!");	
	}	
	catch (Exception $ex)
	{
		$eWin = new jqUI_errorNotify($ex);
		$js->error = $eWin->render();
	}
	
	echo $js->render();
	exit(0);
}
